<p id="notice"><%= notice %></p>

<h1>Bodeguero Obras</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bodeguero_obras.each do |bodeguero_obra| %>
      <tr>
        <td><%= link_to 'Show', bodeguero_obra %></td>
        <td><%= link_to 'Edit', edit_bodeguero_obra_path(bodeguero_obra) %></td>
        <td><%= link_to 'Destroy', bodeguero_obra, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

