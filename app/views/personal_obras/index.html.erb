<p id="notice"><%= notice %></p>

<h1>Personal Obras</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personal_obras.each do |personal_obra| %>
      <tr>
        <td><%= link_to 'Show', personal_obra %></td>
        <td><%= link_to 'Edit', edit_personal_obra_path(personal_obra) %></td>
        <td><%= link_to 'Destroy', personal_obra, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

