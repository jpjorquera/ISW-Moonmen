<p id="notice"><%= notice %></p>

<h1>Inventario Obras</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inventario_obras.each do |inventario_obra| %>
      <tr>
        <td><%= link_to 'Show', inventario_obra %></td>
        <td><%= link_to 'Edit', edit_inventario_obra_path(inventario_obra) %></td>
        <td><%= link_to 'Destroy', inventario_obra, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inventario Obra', new_inventario_obra_path %>
